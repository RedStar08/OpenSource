library verilog;
use verilog.vl_types.all;
entity ALU is
    port(
        in0             : in     vl_logic_vector(15 downto 0);
        in1             : in     vl_logic_vector(15 downto 0);
        alu_op          : in     vl_logic_vector(2 downto 0);
        Z               : out    vl_logic_vector(15 downto 0)
    );
end ALU;
